{
  "name": "gitlab-notice",
  "displayName": "Gitlab Notice",
  "description": "",
  "version": "0.0.1",
  "publisher": "Patrick",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "gitlabNotice",
          "title": "Gitlab Notice",
          "icon": "media/gitlab.svg"
        }
      ]
    },
    "views": {
      "gitlabNotice": [
        {
          "id": "gitlabCI",
          "name": "Gitlab CI/CD"
        },
        {
          "id": "gitlabMR",
          "name": "Gitlab MR"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "gitlabNotice.ci.refresh",
          "when": "view === gitlabCI",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {}
    ],
    "configuration": [
      {
        "title": "Gitlab Config",
        "properties": {
          "gitlabNotice.gitlabUrl": {
            "type": "string",
            "default": ""
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "axios": "^0.18.0"
  }
}
